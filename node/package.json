{
  "name": "unity-embedded-app-sdk",
  "version": "0.0.1",
  "description": "Open source base project to use Unity with the Discord's Embedded App SDK to create activities!",
  "scripts": {
    "unity": "node ./src/scripts/build_bridge_lib.js",
    "build": "npx tsc && node ./src/scripts/build.js",
    "execute": "node build/server/index.js",
    "start": "npm run build && npm run execute"
  },
  "author": "Furnyr",
  "license": "CC0-1.0",
  "dependencies": {
    "@discord/embedded-app-sdk": "^1.0.0",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.3",
    "esbuild": "0.17.11",
    "eventemitter3": "^5.0.0",
    "express": "^4.18.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.4",
    "@types/uuid": "^9.0.8",
    "glob": "^9.2.1",
    "nodemon": "^3.1.0",
    "typescript": "^5.2.2"
  }
}
